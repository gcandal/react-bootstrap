{
    "name": "react-bootstrap",
    "version": "0.1.0",
    "description": "React Bootstrap",
    "homepage": "https://github.com/gcandal/react-bootstrap#readme",
    "bugs": {
        "url": "https://github.com/gcandal/react-bootstrap/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gcandal/react-bootstrap.git"
    },
    "author": "Gabriel Candal",
    "scripts": {
        "build": "webpack --mode production --config webpack.config.js",
        "build-dev": "webpack --mode development --config webpack.config.js",
        "dev": "webpack-dev-server --config webpack.config.js --mode development --watch --hot",
        "lint": "eslint \"./**/*.{js,ts,tsx}\" --fix",
        "prettier": "prettier \"./**/*.{js,ts,tsx,json}\" --write",
        "pretty": "npm run prettier && npm run lint && npm run stylelint && npm run svgo",
        "stylelint": "stylelint \"./**/*.{js,ts,tsx}\"",
        "svgo": "svgo -r . --disable='removeDimensions,removeViewBox,minifyStyles'",
        "upgrade": "npx sort-package-json && ncu -u",
        "webpack": "webpack --config webpack.config.js",
        "watch": "webpack --config webpack.build.config.js --mode development --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.0",
